{
  "tests": [
    {
      "name": "initial_content",
      "description": "Hover works on original tag name",
      "inputFile": "initial.html",
      "elementPosition": {"line": 6, "character": 5},
      "elementMatch": {
        "tagName": "test-element",
        "range": {
          "start": {"line": 6, "character": 2},
          "end": {"line": 6, "character": 14}
        }
      }
    },
    {
      "name": "with_newline",
      "description": "Hover still works after adding new line above",
      "inputFile": "with-newline.html", 
      "elementPosition": {"line": 7, "character": 5},
      "elementMatch": {
        "tagName": "test-element",
        "range": {
          "start": {"line": 7, "character": 2},
          "end": {"line": 7, "character": 14}
        }
      }
    },
    {
      "name": "multiple_edits",
      "description": "Hover works after multiple edits",
      "inputFile": "multiple-edits.html",
      "elementPosition": {"line": 8, "character": 5},
      "elementMatch": {
        "tagName": "test-element",
        "range": {
          "start": {"line": 8, "character": 2},
          "end": {"line": 8, "character": 14}
        },
        "attributes": {
          "test-attr": {
            "name": "test-attr",
            "value": "hello",
            "range": {
              "start": {"line": 8, "character": 15},
              "end": {"line": 8, "character": 33}
            }
          }
        }
      },
      "attributePosition": {"line": 8, "character": 25}
    }
  ]
}